{
  "name": "feedbackfruits-knowledge-engine",
  "version": "1.3.3",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "prestart": "npm run build",
    "start": "node lib",
    "pretest": "npm run clean && tsc --inlineSourceMap --outDir lib",
    "test": "nyc ava --verbose",
    "posttest": "npm run clean && npm run build",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "watch": "nodemon lib",
    "lint": "tslint -c tslint.json -p tsconfig.json --type-check -t codeFrame 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "nyc": {},
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Steffan Sluis <steffansluis@gmail.com>",
  "repository": "git+https://github.com/feedbackfruits/feedbackfruits-knowledge-engine.git",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/feedbackfruits/feedbackfruits-knowledge-engine/issues"
  },
  "homepage": "https://github.com/feedbackfruits/feedbackfruits-knowledge-engine#readme",
  "dependencies": {
    "@types/n3": "^0.0.3",
    "@types/node": "^8.0.14",
    "dotenv": "^4.0.0",
    "feedbackfruits-knowledge-context": "feedbackfruits/feedbackfruits-knowledge-context#^1.1.5",
    "isuri": "^2.0.3",
    "jsonld": "^0.5.18",
    "memux": "^0.4.5",
    "n3": "^0.11.2",
    "rdf-canonize": "^0.2.3",
    "rdf-nx-parser": "^1.0.3",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "blue-tape": "^0.2.0",
    "codecov": "^1.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "sinon": "^4.2.2",
    "supertest": "^3.0.0",
    "tap": "^5.7.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
